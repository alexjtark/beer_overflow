<div id="signup_form">
  <%= form_for(:user, url: signup_path) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>

    <%= f.submit "Signup" %>
  <% end %>
</div>